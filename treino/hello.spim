            .data
hello:      .asciiz "hello world"


            .text
main:
            add $t0, $t0, $zero
            
            li $v0, 5
            syscall
            move $t1, $v0

loop:
            beq $t0, $t1, end

            li $v0, 4
            la $a0, hello
            syscall

            li $v0, 11
            li $a0, 10
            syscall

            addi $t0, $t0, 1

            j loop


end:        
            li $v0, 10
            syscall